-- Create the main UI frame
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local InfiniteYieldButton = Instance.new("TextButton")
local ResetButton = Instance.new("TextButton")
local DcRoomButton = Instance.new("TextButton")
local TweenService = game:GetService("TweenService")

-- Set properties for ScreenGui
ScreenGui.Name = "CustomUI"
ScreenGui.Parent = game.CoreGui

-- Set properties for MainFrame
MainFrame.Name = "MainFrame"
MainFrame.Parent = ScreenGui
MainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
MainFrame.Position = UDim2.new(0.5, -100, 0.5, -75)
MainFrame.Size = UDim2.new(0, 200, 0, 150)
MainFrame.Active = true
MainFrame.Draggable = true

-- Set properties for UICorner to make smooth edges
UICorner.CornerRadius = UDim.new(0, 10)
UICorner.Parent = MainFrame

-- Set properties for InfiniteYieldButton
InfiniteYieldButton.Name = "InfiniteYieldButton"
InfiniteYieldButton.Parent = MainFrame
InfiniteYieldButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
InfiniteYieldButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
InfiniteYieldButton.Position = UDim2.new(0.1, 0, 0.1, 0)
InfiniteYieldButton.Size = UDim2.new(0.8, 0, 0.2, 0)
InfiniteYieldButton.Font = Enum.Font.SourceSans
InfiniteYieldButton.Text = "Infinite Yield"
InfiniteYieldButton.TextColor3 = Color3.fromRGB(255, 255, 255)
InfiniteYieldButton.TextSize = 14

-- Set properties for ResetButton
ResetButton.Name = "ResetButton"
ResetButton.Parent = MainFrame
ResetButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ResetButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
ResetButton.Position = UDim2.new(0.1, 0, 0.4, 0)
ResetButton.Size = UDim2.new(0.8, 0, 0.2, 0)
ResetButton.Font = Enum.Font.SourceSans
ResetButton.Text = "Reset"
ResetButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ResetButton.TextSize = 14

-- Set properties for DcRoomButton
DcRoomButton.Name = "DcRoomButton"
DcRoomButton.Parent = MainFrame
DcRoomButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
DcRoomButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
DcRoomButton.Position = UDim2.new(0.1, 0, 0.7, 0)
DcRoomButton.Size = UDim2.new(0.8, 0, 0.2, 0)
DcRoomButton.Font = Enum.Font.SourceSans
DcRoomButton.Text = "DC Room"
DcRoomButton.TextColor3 = Color3.fromRGB(255, 255, 255)
DcRoomButton.TextSize = 14

-- Function to open Infinite Yield
InfiniteYieldButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

-- Function to reset the avatar
ResetButton.MouseButton1Click:Connect(function()
    game.Players.LocalPlayer.Character:BreakJoints()
end)

-- Function to open DC Room
DcRoomButton.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/LolnotaKid/Obfuscate2/main/obf_TOh95bwY3QlD6Su0tvByE4lEBi14b4R54ousQ9R8vTAjGjF0ee7wts7he0gZh4Ib.lua.txt"))()
end)

-- Function to reset the avatar when 'R' key is pressed
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.R and not gameProcessed then
        game.Players.LocalPlayer.Character:BreakJoints()
    end
end)

-- Tween to blend in the UI slowly
local tweenInfo = TweenInfo.new(3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local tween = TweenService:Create(MainFrame, tweenInfo, {BackgroundTransparency = 0})
MainFrame.BackgroundTransparency = 1

-- Tween to blend in the buttons slowly
local buttonTweenInfo = TweenInfo.new(3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local buttonTween1 = TweenService:Create(InfiniteYieldButton, buttonTweenInfo, {BackgroundTransparency = 0, TextTransparency = 0})
local buttonTween2 = TweenService:Create(ResetButton, buttonTweenInfo, {BackgroundTransparency = 0, TextTransparency = 0})
local buttonTween3 = TweenService:Create(DcRoomButton, buttonTweenInfo, {BackgroundTransparency = 0, TextTransparency = 0})
InfiniteYieldButton.BackgroundTransparency = 1
InfiniteYieldButton.TextTransparency = 1
ResetButton.BackgroundTransparency = 1
ResetButton.TextTransparency = 1
DcRoomButton.BackgroundTransparency = 1
DcRoomButton.TextTransparency = 1

-- Play the tweens
tween:Play()
buttonTween1:Play()
buttonTween2:Play()
buttonTween3:Play()

-- Function to reset the avatar's health to 0
local function resetCharacter()
    local character = game.Players.LocalPlayer.Character
    if character and character:FindFirstChild("Humanoid") then
        character.Humanoid.Health = 0
    end
end

ResetButton.MouseButton1Click:Connect(resetCharacter)

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.R and not gameProcessed then
        resetCharacter()
    end
end)

-- Toggle UI visibility with 'T' key
local uiVisible = true
game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.T and not gameProcessed then
        uiVisible = not uiVisible
        MainFrame.Visible = uiVisible
    end
end)
